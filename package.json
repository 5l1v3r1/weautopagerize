{
  "name": "weautopagerize",
  "description": "Automatically inserts the next page.",
  "version": "1.2.2",
  "author": "wantora <wantora@gmail.com>",
  "bugs": {
    "url": "https://github.com/wantora/weautopagerize/issues"
  },
